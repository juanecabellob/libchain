

/**
 * The User entity.
 */
entity LibUser {
	/**
	* The firstname attribute.
	*/
	firstName String,
	lastName String,
	email String
}
/**
 * The Library entity.
 */
entity Library {
	name String,
	pubKey String,
	privKey String
}


entity Book {
	title String,
        pubKey String
}

relationship OneToMany{
	Library{member} to LibUser
}

relationship OneToMany{
	LibUser{loan} to Book
}


